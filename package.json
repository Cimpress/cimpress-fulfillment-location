{
  "name": "fulfillment-location-service",
  "version": "0.0.1",
  "author": "LogisticsQuotingandPlanning@cimpress.com",
  "description": "A simple client for TRDLNK's Fulfillment Location service",
  "license": "Apache-2.0",
  "dependencies": {
      
  },
  "scripts": {
    "test": "mocha --recursive tests",
    "check-cover": "nyc check-coverage",
    "cover": "cross-env BABEL_ENV=test nyc ./node_modules/mocha/bin/_mocha --recursive tests && npm run check-cover -- --lines 95",
    "code-check": "eslint --ext .js --ext .jsx src",
    "eslint-report": "eslint --ext .js --ext .jsx . -f node_modules/eslint-html-reporter/reporter.js -o eslint.report.html",
    "eslint": "eslint",
    "build": "npm run code-check && npm run cover"
  },
  "devDependencies": {
    "babel-plugin-istanbul": "^4.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.1",
    "eslint": "^4.2.0",
    "eslint-html-reporter": "^0.5.2",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-exec": "^2.0.0",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3"
  }
}